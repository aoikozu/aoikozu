{
  "name": "discord-music-bot",
  "version": "2.0.3",
  "description": "Simple Music Bot for Discord without any database",
  "private": true,
  "main": "dist/lib.js",
  "types": "typings/lib.d.ts",
  "engines": {
    "node": ">=16.6.0"
  },
  "dependencies": {
    "@discordjs/voice": "^0.13.0",
    "comment-json": "^4.2.3",
