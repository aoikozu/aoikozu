    "opusscript": "0.0.8",
    "play-dl": "^1.9.6",
    "prism-media": "^1.3.4",
    "soundcloud.ts": "^0.4.9",
    "tslib": "^2.4.1",
    "ytdl-core": "^4.11.2",
    "ytpl": "^2.3.0",
    "ytsr": "^3.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@discordjs/builders": "^1.3.0",
    "@discordjs/rest": "^1.3.0",
    "@mtripg6666tdr/commitlint-config": "^1.1.0",
